  "scripts": {
    "clear": "rm -rf dist/ coverage/ .nyc_output/",
    "clear:all": "rm -rf node_modules/ npm-debug.log && npm run clear",
    "test": "jasmine --reporter=jasmine-console-reporter --config=jasmine.json --random=false",
    "test:watch": "nodemon --ext ts --exec 'jasmine --reporter=jasmine-console-reporter --config=jasmine.json --random=false'",
    "coverage": "nyc npm run test && nyc report --reporter=html",
    "coveralls": "nyc npm run test && nyc report --reporter=text-lcov | coveralls -v",
    "broadcast": "sls invoke --function broadcast --data '{ \"body\": \"Hello from server\" }'",
    "localbroadcast": "sls invoke local --function broadcast --data '{ \"body\": \"Hello from server\" }'",
    "localbackend": "sls offline start --inMemory --stage dev",
    "webstart": "PORT=3002 react-scripts start",
    "webbuild": "react-scripts build",
    "webtest": "react-scripts test",
    "webeject": "react-scripts eject"
  },
  "devDependencies": {
    "@anttiviljami/serverless-stack-output": "0.3.1",
